{
    "name": "__MSG_name__",
    "description": "__MSG_pluginDesc__",
    "version": "0.0.6",
    "manifest_version": 3,
    "action": 
    {
        "default_icon":"Timer.png",
        "default_popup":"main.html"
    },
    "icons":
    {
        "256":"Timer.png"
    },
    "background": {
        "service_worker": "Service/service.js"
    },
    "permissions":
    [
        "alarms",
        "notifications",
        "storage",
        "background",
        "contextMenus"
    ],
    "default_locale":"zh_CN",
    "content_scripts": [
        {
            "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": [
            "popup/content_js.js"
        ]
    }]
}